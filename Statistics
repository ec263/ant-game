    public int blackCarryFood;
    
    public Statistics(){
        redName="";
        blackName="";
        redAnts=0;
        blackAnts=0;
        redKills=0;
        blackKills=0;
        redHillFood=0;
        blackHillFood=0;
        mapFood=0;
        redCarryFood=0;
        blackCarryFood=0;
    }
}
